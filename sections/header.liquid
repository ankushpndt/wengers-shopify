{%- style -%}
  :root {
    --color-trans-text : {{ section.settings.trans_text.red }},{{ section.settings.trans_text.green }},{{ section.settings.trans_text.blue }}
  }
  header-drawer {
    justify-self: start;
  }
  {%- if section.settings.head_shadow == 'head_shadow_all' -%}
  sticky-header.header-wrapper {
    box-shadow: 0 4px 20px rgba(var(--color-shadow),.08)
  }
  {%- endif -%}
  {%- if section.settings.head_shadow == 'head_shadow_home' -%}
  sticky-header.header-wrapper {
    box-shadow: 0 4px 20px rgba(var(--color-shadow),.08)
  }
  .page-index .header-wrapper {
    box-shadow: none;
  }
  {%- endif -%}
  .shopify-section-header-sticky.scrolled-past-header sticky-header.header-wrapper {
    box-shadow: 0 4px 20px rgba(var(--color-shadow),.08)
  }

  .header__icon .icon:not(.icon-close),
  .wbhcart svg,.userdrop svg,.header__search .header__icon svg{
    width: {{ section.settings.iconwidth_header }}px;
    height: {{ section.settings.iconwidth_header }}px;
  } 
  .wbheader_cont svg {
    width: {{ section.settings.iconwidth }}px;
    height: {{ section.settings.iconwidth }}px;
  }
  {% if section.settings.display_contact_bg == true %}
    .page-index .transparent_header .wbheader_cont {
      background: rgba(var(--color-trans-text),0.1);
    }
    .wbheader_cont {
      background: rgba(var(--color-foreground),0.1);
      padding: 10px 15px;
      border-radius: calc(var(--text-boxes-radius) / 2);
    }
  {% endif %}
  .slideshow .banner__content{
    padding: 70px 20px 90px 20px;
  }
  @media (max-width: 767px) {
    .slideshow .banner__content {
      padding: 70px 20px 30px 20px; 
    }
  }
  {% if section.settings.transparent_header == true %}
    @media screen and (min-width: 992px){
      .slideshow .banner__content{
        padding-top: 200px;
      }
    }
  {% endif %}
  header-drawer {
    justify-self: start;
  }
  {%- if section.settings.logo_position == 'top-left' -%}
    .psearch-hidden {
      overflow: hidden;
    }
    .psearch-hidden:before{
      content: '';
      position: fixed;
      z-index: 3;
      left: 0;
      top: 0;
      width: 100vw;
      height: 100%;
      display: flex;
      justify-content: flex-end;
      background-color: rgba(var(--color-foreground),.3);
    }
    .psearch-hidden .section-header{
      position: relative;
    }
    .psearch-hidden .header--top-left .header__search {
      z-index: 3;
    }
    .psearch-hidden .section-header.shopify-section-header-sticky{
      position: sticky;
    }
  {%- endif -%}

  {%- if section.settings.sticky_header_type == 'reduce-logo-size' -%}
    .scrolled-past-header .header__heading-logo-wrapper {
      width: 75%;
    }
  {%- endif -%}
  /* @media screen and (min-width: 992px) {
    header-drawer.mobile-drawer {
      display: none;
    }
  } */

  {%- if section.settings.menu == blank -%}
    @media screen and (min-width: 992px) {
      .gradient.wbmenuup.header__inline-menu {
        padding: 0;
      }
    }
  {%- endif -%}


  .header{
    padding-top: {{ section.settings.padding_top | times: 0.5 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.5 | round: 0 }}px;
  }

  .header__leftpart{
    display: flex;
    align-items: center;
    gap: 1rem;
  }

  @media screen and (max-width: 991px) {
    .header.header--top-left details-modal {
      margin-top: {{ section.settings.padding_top | times: 0.5 | round: 0 }}px;
    }
  }
  .section-header {
    position: sticky;
  }
  .section-header .header-wrapper {
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }
  predictive-search:not([loading]) .predictive-search__loading-state, predictive-search:not([loading]) .predictive-search-status__loading {
      display: none;
  }
  @media screen and (min-width: 768px) {
    .section-header .header-wrapper {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }

  @media screen and (min-width: 992px) {
    .header{
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}
