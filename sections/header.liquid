{%- liquid
  for block in section.blocks
    if block.type == '@app'
      assign has_app_block = true
    endif
    if block.type == 'second-menu'
      assign secondary__menu = block.settings.secmenu
    endif
  endfor
-%}

<{% if section.settings.sticky_header_type != 'none' %}sticky-header data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %}  class="{% if section.settings.transparent_header == true %} tranhead transparent_header{% endif %}  header-wrapper color-{{ section.settings.color_schemehead }} gradient dropm-menu {% if section.settings.border_bottom == true %} header-wrapper--border-bottom{% endif %} {% if section.settings.transparent_header == true and section.settings.trans_header_bg == true %} trans_head_bg{% endif %}" data-trans="{{  section.settings.transparent_header }}" data-trans_header_bg="{{  section.settings.trans_header_bg }}">
<div  class="htop--{{ section.settings.logo_position }} {% if section.settings.header_full != true %}page-width {% else %}around_padding {% endif %}">
  <header class="header header--{{ section.settings.logo_position }} {% if section.settings.logo_position == 'middle-center' and section.settings.menu == blank %} search-full-width{% endif %} header--mobile-left{% if section.settings.menu != blank %} header--has-menu{% endif %}{% if has_app_block %} header--has-app{% endif %}{% if settings.wb_cartnumber == 'wb_shoptotal' or settings.wb_cartnumber == 'wb_shopqtytotal' %} wbcartbth{% endif %}">
    <div class="header__leftpart">  
    <header-drawer data-breakpoint="tablet" class="mobile-drawer">
        <details id="Details-menu-drawer-container" class="menu-drawer-container">
          <summary class="header__icon header__icon--menu header__icon--summary link focus-inset" aria-label="{{ 'sections.header.menu' | t }}">
            <span>
              {% render 'icon-hamburger' %}
              {% render 'icon-close' %}
            </span>
          </summary>
            <div id="menu-drawer" class="color-{{ section.settings.color__drop_menu }} gradient menu-drawer motion-reduce" tabindex="-1">
              <div class="mobile_menu_tab menu-drawer__inner-container">
                <div class="menu-drawer__navigation-container">
                  <nav class="menu-drawer__navigation">
                    <div class="sticky-menu-drawer_links">
                      {%- if section.settings.enable_country_selector or section.settings.enable_language_selector -%}
                        <div class="menu-drawer__localization header__localization">
                          {%- if section.settings.enable_language_selector and localization.available_languages.size > 1 -%}
                            <localization-form>
                              {%- form 'localization', id: 'HeaderLanguageMobileForm', class: 'localization-form' -%}
                                <div class="no-js-hidden">
                                  {%- render 'language-localization', localPosition: 'HeaderLanguageMobile' -%}
                                </div>
                              {%- endform -%}
                            </localization-form>
                          {%- endif -%}
                          {%- if section.settings.enable_country_selector and localization.available_countries.size > 1 -%}
                            <localization-form>
                              {%- form 'localization', id: 'HeaderCountryMobileForm', class: 'localization-form' -%}
                                <div class="no-js-hidden">
                                  {%- render 'country-localization', localPosition: 'HeaderCountryMobile' -%}
                                </div>
                              {%- endform -%}
                            </localization-form>
                          {% endif %}
                        </div>
                      {%- endif -%}
                    </div>

                    <ul class="menu-drawer__menu has-submenu list-menu">
                      {%- for link in section.settings.menu.links -%}
                        {%- liquid 
                           assign assignsubmenu = true
                         assign blank_link = false
                         assign onlymenulink = false
                         for block in section.blocks
                             assign case_title = block.settings.menuname | downcase
                             assign lower_title = link.title | downcase
                             if lower_title == case_title
                                 if block.settings.showsubmenu == false
                                     assign assignsubmenu = false
                                     assign onlymenulink = true
                                 endif
                                 if link.links == blank 
                                     assign blank_link = lower_title
                                 endif
                             endif
                         endfor 
                        -%}
                        <li>
                          
                          {%- if link.links != blank -%}
                            {% if onlymenulink %}
                              <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                {{ link.title | escape }}
                                {% for block in section.blocks %}
                                  {% if block.type == 'menu-badge' %}
                                    {% liquid
                                      assign badge_case_title = block.settings.badgemenuname | downcase
                                    %}
                                  {% if lower_title == badge_case_title %}
                                    {% if block.settings.badge.size > 0 %}
                                      <span class="menu_badge menu_badge--{{ block.id }}" style="background-color: {{ block.settings.badge_bg }}; color:{{ block.settings.badge_text }};">{{ block.settings.badge }}</span>
                                    {% endif %}
                                  {% endif %} 
                                  {% style %}
                                    .menu_badge--{{ block.id }}::before {
                                        border-bottom-color: {{ block.settings.badge_bg }};
                                    }
                                  {% endstyle %}
                                  {% endif %}
                                {% endfor %}
                              </a>
                            {% endif %}
                            {% if assignsubmenu %}
                              <details id="Details-menu-drawer-menu-item-{{ forloop.index }}">
                                <summary class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--active{% endif %}">
                                  {{ link.title | escape }}
                                  {% for block in section.blocks %}
                                    {% if block.type == 'menu-badge' %}
                                      {% liquid
                                        assign badge_case_title = block.settings.badgemenuname | downcase
                                      %}
                                    {% if lower_title == badge_case_title %}
                                      {% if block.settings.badge.size > 0 %}<span class="menu_badge menu_badge--{{ block.id }}" style="background-color: {{ block.settings.badge_bg }}; color:{{ block.settings.badge_text }};">{{ block.settings.badge }}</span>{% endif %}
                                    {% endif %} 
                                    {% style %}
                                      .menu_badge--{{ block.id }}::before {
                                          border-bottom-color: {{ block.settings.badge_bg }};
                                      }
                                    {% endstyle %}
                                    {% endif %}
                                  {% endfor %}
                                  {% render 'icon-arrow' %}
                                  {% render 'icon-caret' %}
                                </summary>
                                <div id="link-{{ link.handle | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce" tabindex="-1">
                                  <div class="menu-drawer__inner-submenu">
                                    <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                      {% render 'icon-arrow' %}
                                      {{ link.title | escape }}
                                    </button>
                                    <ul class="menu-drawer__menu list-menu" tabindex="-1">
                                      {% if assignsubmenu %}
                                        {%- for childlink in link.links -%}
                                          <li>
                                            {%- if childlink.links == blank -%}
                                              <a href="{{ childlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if childlink.current %} menu-drawer__menu-item--active{% endif %}"{% if childlink.current %} aria-current="page"{% endif %}>
                                                {{ childlink.title | escape }}
                                              </a>
                                            {%- else -%}
                                              <details>
                                                <summary class="menu-drawer__menu-item link link--text list-menu__item focus-inset">
                                                  {{ childlink.title | escape }}
                                                  {% render 'icon-arrow' %}
                                                  {% render 'icon-caret' %}
                                                </summary>
                                                <div id="childlink-{{ childlink.handle | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce">
                                                  <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                                    {% render 'icon-arrow' %}
                                                    {{ childlink.title | escape }}
                                                  </button>
                                                  <ul class="menu-drawer__menu list-menu" tabindex="-1">
                                                    {%- for grandchildlink in childlink.links -%}
                                                      <li>
                                                        <a href="{{ grandchildlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if grandchildlink.current %} menu-drawer__menu-item--active{% endif %}"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                                                          {{ grandchildlink.title | escape }}
                                                        </a>
                                                      </li>
                                                    {%- endfor -%}
                                                  </ul>
                                                </div>
                                              </details>
                                            {%- endif -%}
                                          </li>
                                        {%- endfor -%}
                                      {% endif %}
                                      {% render 'megamenu', blocks: section.blocks, title: link.title %}
                                    </ul>
                                  </div>
                                </div>
                              </details>
                            {% endif %}
                          {%- else -%}
                            {% if blank_link %}
                              {% if onlymenulink %}
                                <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                {{ link.title | escape }}
                                {% for block in section.blocks %}
                                  {% if block.type == 'menu-badge' %}
                                    {% liquid
                                      assign badge_case_title = block.settings.badgemenuname | downcase
                                    %}
                                  {% if lower_title == badge_case_title %}
                                    {% if block.settings.badge.size > 0 %}<span class="menu_badge menu_badge--{{ block.id }}" style="background-color: {{ block.settings.badge_bg }}; color:{{ block.settings.badge_text }};">{{ block.settings.badge }}</span>{% endif %}
                                  {% endif %} 
                                  {% style %}
                                    .menu_badge--{{ block.id }}::before {
                                        border-bottom-color: {{ block.settings.badge_bg }};
                                    }
                                  {% endstyle %}
                                  {% endif %}
                                {% endfor %}
                                </a>
                              {% endif %}
                              {% if assignsubmenu %}
                                <details id="Details-menu-drawer-menu-item-{{ forloop.index }}">
                                    <summary class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--active{% endif %}">
                                      {{ link.title | escape }}
                                      {% for block in section.blocks %}
                                        {% if block.type == 'menu-badge' %}
                                          {% liquid
                                            assign badge_case_title = block.settings.badgemenuname | downcase
                                          %}
                                        {% if lower_title == badge_case_title %}
                                          {% if block.settings.badge.size > 0 %}<span class="menu_badge menu_badge--{{ block.id }}" style="background-color: {{ block.settings.badge_bg }}; color:{{ block.settings.badge_text }};">{{ block.settings.badge }}</span>{% endif %}
                                        {% endif %} 
                                        {% style %}
                                          .menu_badge--{{ block.id }}::before {
                                              border-bottom-color: {{ block.settings.badge_bg }};
                                          }
                                        {% endstyle %}
                                        {% endif %}
                                      {% endfor %}
                                      {% render 'icon-arrow' %}
                                      {% render 'icon-caret' %}
                                    </summary>
                                    <div id="link-{{ link.title | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce" tabindex="-1">
                                      <div class="menu-drawer__inner-submenu">
                                          <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                            {% render 'icon-arrow' %}
                                            {{ link.title | escape }}
                                          </button>
                                          <div class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                            {% render 'megamenu', blocks: section.blocks, title: link.title %}
                                          </div>
                                      </div>
                                    </div>
                                </details>
                              {% endif %}
                                {% assign blank_link = false %}
                              {% else %}
                                <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                {{ link.title | escape }}
                                {% for block in section.blocks %}
                                  {% if block.type == 'menu-badge' %}
                                    {% liquid
                                      assign badge_case_title = block.settings.badgemenuname | downcase
                                    %}
                                  {% if lower_title == badge_case_title %}
                                    {% if block.settings.badge.size > 0 %}<span class="menu_badge menu_badge--{{ block.id }}" style="background-color: {{ block.settings.badge_bg }}; color:{{ block.settings.badge_text }};">{{ block.settings.badge }}</span>{% endif %}
                                  {% endif %} 
                                  {% style %}
                                    .menu_badge--{{ block.id }}::before {
                                        border-bottom-color: {{ block.settings.badge_bg }};
                                    }
                                  {% endstyle %}
                                  {% endif %}
                                {% endfor %}
                                </a>
                            {% endif %}
                          {%- endif -%}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </nav>

                  <div class="menu-drawer__utility-links">
                    {%- for block in section.blocks -%}
                      {%- if block.settings.secmenu != blank -%}
                        <nav class="wbsecmenu secondary-menu-{{ block.id }} {% if section.settings.menu == blank %} wbsecmenupad{% endif %}">
                          <ul class="list-menu list-menu--inline">
                          {%- for link in block.settings.secmenu.links -%}
                              <li>
                                <a href="{{ link.url }}">
                                  <span>{{ link.title | escape }}</span>
                                </a>
                              </li>
                          {%- endfor -%}
                          </ul>
                        </nav>
                      {%- endif -%}
                      {%- style -%}
                        .secondary-menu-{{ block.id }} ul li:first-child a{
                          color: {{ block.settings.first_color }};
                        }
                        @media (min-width: 992px) {
                          .page-index .transparent_header .wbsecmenu ul li a { 
                            color: rgba(var(--color-trans-text));
                          }
                          .page-index .transparent_header .wbsecmenu.secondary-menu-{{ block.id }} ul li:first-child a{
                            color: {{ block.settings.first_color }}; 
                          }
                        }
                      {%- endstyle -%}
                    {% endfor %}

                    {%- if section.settings.display_contact == true -%}
                      <div class="cont_mleft">
                        <ul class="wbheader_cont">
                          <li><span class="multis-svg">{% render 'icon-allservice', icon: section.settings.contact_icon %}</span></li>
                          <li>
                            {%- if section.settings.contact_title != blank -%}
                              <h2>{{ section.settings.contact_title }}</h2>
                            {%- endif -%}
                            {%- if section.settings.contact_text != blank -%}
                              <p>
                                {%- if section.settings.contact_link != blank -%}<a href="{{ section.settings.contact_link }}">{%- endif -%}
                                {{ section.settings.contact_text }}
                                {%- if section.settings.contact_link != blank -%}</a>{%- endif -%}
                              </p>
                            {%- endif -%}
                          </li>
                        </ul>
                      </div>
                    {%- endif -%}
                  
                  </div>
                
                </div>
              </div>

            </div>
        </details>
      </header-drawer>
      {%- if section.settings.logo_position == 'top-center' -%}
        <div class="leftpart_contact">
